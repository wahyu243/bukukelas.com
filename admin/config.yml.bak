backend:
  name: git-gateway
  branch: main
  
logo_url: https://administrasi.net/svg/logo.svg
  
media_folder: "img/"
public_folder: "/img"

publish_mode: editorial_workflow
collections:
  - name: "artikel"
    label: "Artikel"
    folder: "_posts/artikel"
    create: true
    slug: "{{year}}-{{month}}-{{day}}-{{slug}}"
    identifier_field: name
    fields:
      - {label: "Layout", name: "layout", widget: "hidden", default: "post"}
      - {label: "Title", name: "title", widget: "string"}
      - {label: "Description", name: "description", widget: "string"}
      - label: "Image"
        name: "image"
        widget: "image"
        required: false
      - {label: "Date", name: "date", widget: "datetime"}
      - label: "Author"
        name: "author"
        widget: "string"
        required: false
      - {label: "Body", name: "body", widget: "markdown"}
      - {label: "Tags", name: "tags", hint: "Contoh cpns, guru, pendidikan", widget: "list"}
      - label: "Nama Sumber"
        name: "nama_sumber_1"
        widget: "string"
        required: false
      - label: "Link Sumber"
        name: "link_sumber_1"
        widget: "string"
        required: false
      - label: "Sumber Gambar"
        name: "name_img_source"
        widget: "string"
        required: false
      - label: "Link Gambar"
        name: "link_img"
        widget: "string"
        required: false

